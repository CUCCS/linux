$TTL	604800
@	IN	SOA	localhost. root.localhost. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
	IN	NS	ns.cuc.edu.cn.
ns	IN	A       {{ INTERFACE_ADDRESS }} 
wp.sec.cuc.edu.cn	IN	A	{{ INTERFACE_ADDRESS }}
dvwa.sec.cuc.edu.cn	IN	CNAME	wp.sec.cuc.edu.cn.
